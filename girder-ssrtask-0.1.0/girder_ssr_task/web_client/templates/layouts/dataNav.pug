.appsNavPanel.panel
  li.g-global-nav-li(data-toggle="collapse", data-target='#appsSAIPArch', data-parent="#appsNav")
    a.g-nav-link.qc-SAIP(g-name='qcSAIPProject', g-target='qcSAIPProject')
      i.icon-download-cloud
      span SAIP
  #appsSAIPArch.appsArch.collapse

if SSR_Project.id
  .appsNavPanel.panel
    li.g-global-nav-li(data-toggle="collapse", data-target='#appsCollectionArch', data-parent="#appsNav")
      a.g-nav-link.qc-Collection(g-id=SSR_Project.id, g-name='qcSSRProject', g-target='qcSSRProject')
        i.icon-file-archive
        span #{SSR_Project.get('name')}
    #appsCollectionArch.appsArch.collapse

if user.id
  .appsNavPanel.panel
    li.g-global-nav-li(data-toggle="collapse", data-target='#appsUSERArch', data-parent="#appsNav")
      a.g-nav-link.qc-User(g-id=user.id, g-name='qcUsers', g-target='qcUsers')
        i.icon-edit
        span #{user.name()}
    #appsUSERArch.appsArch.collapse
