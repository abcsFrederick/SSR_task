{
    "name": "@girder/ssrtask",
    "version": "0.1.0",
    "description": "A general purpose plugin for SSR task remote task execution.",
    "homepage": "https://github.com/abcsFrederick/SSR_task",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/abcsFrederick/SSR_task"
    },
    "scripts": {
        "lint": "eslint . && pug-lint . && stylint"
    },
    "peerDependencies": {
        "@girder/core": "*",
        "@girder/jobs": "*",
        "@girder/archive": "*",
        "@girder/histomicsui": "*"
    },
    "dependencies": {
        "jquery-ui": "^1.12.1",
        "jquery-ui-bundle": "^1.12.1-migrate"
    },
    "devDependencies": {
        "@girder/eslint-config": "*",
        "@girder/pug-lint-config": "*",
        "eslint": "^5",
        "eslint-config-semistandard": "^13",
        "eslint-config-standard": "^12",
        "eslint-plugin-backbone": "*",
        "eslint-plugin-import": "*",
        "eslint-plugin-node": "*",
        "eslint-plugin-promise": "*",
        "eslint-plugin-standard": "*",
        "eslint-plugin-underscore": "*",
        "pug-lint": "^2",
        "stylint": "^2"
    },
    "main": "./index.js",
    "girderPlugin": {
        "name": "ssrtask",
        "main": "./main.js",
        "dependencies": [
            "archive",
            "histomicsui"
        ],
        "webpack": "webpack.helper"
    },
    "eslintConfig": {
        "extends": "@girder",
        "rules": {
            "for-direction": "error",
            "getter-return": "error",
            "multiline-ternary": [
                "error",
                "always-multiline"
            ],
            "no-alert": "error",
            "switch-colon-spacing": "error",
            "object-curly-spacing": "off",
            "import/exports-last": "error",
            "promise/no-native": "error",
            "promise/no-return-in-finally": "error",
            "promise/no-return-wrap": "error",
            "no-var": "off"
        },
        "root": true
    },
    "pugLintConfig": {
        "extends": "@girder/pug-lint-config"
    },
    "stylintrc": {
        "blocks": false,
        "brackets": {
            "expect": "never",
            "error": true
        },
        "colons": {
            "expect": "never",
            "error": true
        },
        "colors": false,
        "commaSpace": {
            "expect": "always",
            "error": true
        },
        "commentSpace": {
            "expect": "always",
            "error": true
        },
        "cssLiteral": {
            "expect": "never",
            "error": true
        },
        "depthLimit": false,
        "duplicates": {
            "expect": true,
            "error": true
        },
        "efficient": {
            "expect": "always",
            "error": true
        },
        "exclude": [
            "node_modules/**"
        ],
        "extendPref": "@extend",
        "globalDupe": false,
        "groupOutputByFile": {
            "expect": true,
            "error": true
        },
        "indentPref": {
            "expect": 2,
            "error": true
        },
        "leadingZero": {
            "expect": "always",
            "error": true
        },
        "maxErrors": false,
        "maxWarnings": false,
        "mixed": false,
        "mixins": [],
        "namingConvention": false,
        "namingConventionStrict": false,
        "none": {
            "expect": "always",
            "error": true
        },
        "noImportant": false,
        "parenSpace": {
            "expect": "never",
            "error": true
        },
        "placeholders": false,
        "prefixVarsWithDollar": {
            "expect": "always",
            "error": true
        },
        "quotePref": {
            "expect": "double",
            "error": true
        },
        "reporterOptions": {
            "columns": [
                "lineData",
                "severity",
                "description",
                "rule"
            ],
            "columnSplitter": "  ",
            "showHeaders": false,
            "truncate": true
        },
        "semicolons": {
            "expect": "never",
            "error": true
        },
        "sortOrder": false,
        "stackedProperties": {
            "expect": "never",
            "error": true
        },
        "trailingWhitespace": {
            "expect": "never",
            "error": true
        },
        "universal": {
            "expect": "never",
            "error": true
        },
        "valid": {
            "expect": true,
            "error": true
        },
        "zeroUnits": {
            "expect": "never",
            "error": true
        },
        "zIndexNormalize": false
    }
}