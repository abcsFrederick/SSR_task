.g-analysis-nav-container
  width 40vw
  height calc(100vh - 52px)
  display inline-block
  transition 1s
  position absolute
  left 0
  margin-left 0
  
  .active
    background-color #dcdcdc

  #taskNav, #mappingNav
    height calc(50% - 26px)

.task-container
  width 60vw
  float right
  height calc(100vh - 52px)
  transition 1s

.mappingArch
  margin-left 30px
  margin-right 10px
  overflow auto
  max-height 400px
  min-height 300px